<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <data>

        <import type="com.githubyss.mobile.common.kit.app.page.homepage.HomepageViewModel" />

        <variable
            name="homepageVm"
            type="HomepageViewModel"
            />
    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/comres_layoutBg_lightGray"
        android:fillViewport="true"
        android:scrollbars="none"
        >

        <com.google.android.flexbox.FlexboxLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="@dimen/comres_sideMargin_normal"
            android:paddingRight="@dimen/comres_sideMargin_normal"
            android:paddingBottom="@dimen/comres_sideMargin_normal"
            android:paddingLeft="@dimen/comres_sideMargin_normal"
            app:flexDirection="column"
            app:justifyContent="flex_start"
            app:alignItems="center"
            >

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_log"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_log"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_mvvm"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_mvvm"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_mvi"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_mvi"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_binding_reflect"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_view_binding_reflect"
                    />

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_binding_inline"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_view_binding_inline"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_binding_reflect_toolbar"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_view_binding_toolbar_reflect"
                    />

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_binding_inline_toolbar"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_view_binding_toolbar_inline"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/button_json_utils"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_json_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_glide"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_glide"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_lifecycle"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_lifecycle"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_audio_player"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_audio_player"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_contacts_fetch"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_contacts_fetch"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_font"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_font"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_permission"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_permission"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_screenshot_detect"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_screenshot_detect"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_speech_recognition"
                    android:onClick="@{homepageVm::onAnyButtonClick}"
                    android:text="@string/comkit_homepage_button_speech_recognition"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_activity_utils"
                    android:text="@string/comkit_homepage_button_activity_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_adapt_screen_utils"
                    android:text="@string/comkit_homepage_button_adapt_screen_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_app_utils"
                    android:text="@string/comkit_homepage_button_app_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_array_utils"
                    android:text="@string/comkit_homepage_button_array_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_bar_utils"
                    android:text="@string/comkit_homepage_button_bar_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_brightness_utils"
                    android:text="@string/comkit_homepage_button_brightness_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_convert_utils"
                    android:text="@string/comkit_homepage_button_convert_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_datetime_utils"
                    android:text="@string/comkit_homepage_button_datetime_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_device_utils"
                    android:text="@string/comkit_homepage_button_device_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_encrypt_utils"
                    android:text="@string/comkit_homepage_button_encrypt_utils"
                    />
            </com.google.android.flexbox.FlexboxLayout>

            <com.google.android.flexbox.FlexboxLayout style="@style/comres_equant_horizontal_flex_style">

                <Button
                    style="@style/comres_equant_horizontal_button_style"
                    android:id="@+id/btn_"
                    android:text=""
                    />
            </com.google.android.flexbox.FlexboxLayout>
        </com.google.android.flexbox.FlexboxLayout>
    </androidx.core.widget.NestedScrollView>
</layout>
