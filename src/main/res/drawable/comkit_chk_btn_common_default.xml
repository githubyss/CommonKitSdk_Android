<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true"
          android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/comkitCommonChkBtnBgColorDefaultChecked" />
            <corners android:radius="@dimen/comkitCommonChkBtnCornerRadiusDefault" />
            <stroke android:width="@dimen/comkitCommonChkBtnStrokeWidthDefault"
                    android:color="@color/comkitCommonChkBtnStrokeColorDefault" />
        </shape>
    </item>

    <item android:state_checked="false"
          android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/comkitCommonChkBtnBgColorDefaultUnChecked" />
            <corners android:radius="@dimen/comkitCommonChkBtnCornerRadiusDefault" />
            <stroke android:width="@dimen/comkitCommonChkBtnStrokeWidthDefault"
                    android:color="@color/comkitCommonChkBtnStrokeColorDefault" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/comkitCommonChkBtnBgColorDefaultDisabled" />
            <corners android:radius="@dimen/comkitCommonChkBtnCornerRadiusDefault" />
            <stroke android:width="@dimen/comkitCommonChkBtnStrokeWidthDefault"
                    android:color="@color/comkitCommonChkBtnStrokeColorDefault" />
        </shape>
    </item>
</selector>
